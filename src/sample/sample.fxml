<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="sample.Controller" alignment="CENTER" spacing="120" xmlns:fx="http://javafx.com/fxml">
    <VBox.margin>
        <Insets bottom="160"/>
    </VBox.margin>
    <Label fx:id="scoreLabel" text="${score}" id="scoreLabel"/>
    <Button fx:id="orangeButton" id="orangeButton" onAction="#handleButtonClick"/>
</VBox>



